<div class="box">
  <h2>Select New Chair</h2>
  <h2>Chairmanship of the <%= @club_chairmanship.name %></h2>
  <div class="errors"></div>
    <h3>Current Chair(s):</h3>
    <% form_for(@club_chairmanship,
                :url => url_for(:action => :new_chair, :id => @suoc_chairmanship.id),
                :html => { :class => "MainForm" }) do |f| %>
    <%= f.error_messages %>
    <%= f.hidden_field :id %>
    <% if !@current_chairs.empty? %>
      <table>
        <%= render_chair_list(@current_chairs) %>
      </table>
    <% else %>
      There are no current chairs assigned for the chairmanship of the <%= @suoc_chairmanship.name %>.
    <% end %>
    <h3>Select a new chair</h3>
    <p>Type name below to select a new chair.</p>
    <%= text_field_with_auto_complete :suoc_profile, :name %>
    <p>
      <%= f.submit "Update" %>
    </p>
  <% end %>


  <%= link_to 'Club Chairmanships', club_chairmanships_path %> |
  <%= link_to 'Club Activities', club_activities_path %>
</div>
